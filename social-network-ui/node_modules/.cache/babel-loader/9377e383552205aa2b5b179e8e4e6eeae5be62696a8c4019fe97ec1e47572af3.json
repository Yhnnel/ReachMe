{"ast":null,"code":"import router from '@/router';\nimport API from '../api';\nimport { ref } from 'vue';\nexport default {\n  setup() {\n    const userData = ref(null);\n    const fname = ref(null);\n    const mname = ref(null);\n    const lname = ref(null);\n    const bday = ref(null);\n    const gender = ref(null);\n    const pnum = ref(null);\n    const email = ref(null);\n    const pass = ref(null);\n    const confpass = ref(null);\n    const getUserData = async () => {\n      try {\n        let data = {\n          firstname: fname,\n          middlename: mname,\n          lastname: lname,\n          password: confpass,\n          birthday: bday,\n          gender: gender,\n          email: email,\n          phonenumber: pnum\n        };\n        userData.value = await data;\n      } catch (err) {\n        console.log(err);\n      }\n    };\n    getUserData();\n    const submitSignUp = () => {\n      try {\n        if (pass !== confpass) {\n          console.log(\"Password doesn't match\");\n        } else {\n          console.log(userData);\n        }\n      } catch (err) {\n        console.log(err);\n      }\n    };\n    return {\n      fname,\n      mname,\n      lname,\n      bday,\n      gender,\n      pnum,\n      email,\n      pass,\n      confpass,\n      userData\n    };\n  }\n};","map":{"version":3,"mappings":"AA2BA,OAAOA,MAAK,MAAO,UAAU;AAC7B,OAAOC,GAAE,MAAO,QAAQ;AACxB,SAASC,GAAE,QAAS,KAAK;AAEzB,eAAe;EACXC,KAAK,GAAG;IACJ,MAAMC,QAAO,GAAIF,GAAG,CAAC,IAAI;IACzB,MAAMG,KAAI,GAAIH,GAAG,CAAC,IAAI;IACtB,MAAMI,KAAI,GAAIJ,GAAG,CAAC,IAAI;IACtB,MAAMK,KAAI,GAAIL,GAAG,CAAC,IAAI;IACtB,MAAMM,IAAG,GAAIN,GAAG,CAAC,IAAI;IACrB,MAAMO,MAAK,GAAIP,GAAG,CAAC,IAAI;IACvB,MAAMQ,IAAG,GAAIR,GAAG,CAAC,IAAI;IACrB,MAAMS,KAAI,GAAIT,GAAG,CAAC,IAAI;IACtB,MAAMU,IAAG,GAAIV,GAAG,CAAC,IAAI;IACrB,MAAMW,QAAO,GAAIX,GAAG,CAAC,IAAI;IAEzB,MAAMY,WAAU,GAAI,YAAY;MAC5B,IAAG;QACC,IAAIC,IAAG,GAAI;UACPC,SAAS,EAAEX,KAAK;UAChBY,UAAU,EAAEX,KAAK;UACjBY,QAAQ,EAAEX,KAAK;UACfY,QAAQ,EAAEN,QAAQ;UAClBO,QAAQ,EAAEZ