{"ast":null,"code":"import router from '@/router';\nimport API from '../api';\nimport { ref } from 'vue';\nexport default {\n  setup() {\n    const fname = ref(null);\n    const mname = ref(null);\n    const lname = ref(null);\n    const bday = ref(null);\n    const gender = ref(null);\n    const pnum = ref(null);\n    const email = ref(null);\n    const pass = ref(null);\n    const confpass = ref(null);\n    const userData = ref({\n      firstname: null,\n      middlename: null,\n      lastname: null,\n      password: null,\n      birthday: null,\n      gender: null,\n      email: null,\n      phonenumber: null\n    });\n    const getUserData = async () => {\n      try {\n        userData.firstname.value = fname;\n      } catch (err) {\n        console.log(err);\n      }\n    };\n    getUserData();\n    const submitSignUp = () => {\n      try {\n        if (pass != confpass) {\n          console.log(\"Password doesn't match\");\n        } else {\n          console.log(userData.value);\n        }\n      } catch (err) {\n        console.log(err);\n      }\n    };\n    return {\n      fname,\n      mname,\n      lname,\n      bday,\n      gender,\n      pnum,\n      email,\n      pass,\n      confpass,\n      userData,\n      submitSignUp\n    };\n  }\n};","map":{"version":3,"mappings":"AA2BA,OAAOA,MAAK,MAAO,UAAU;AAC7B,OAAOC,GAAE,MAAO,QAAQ;AACxB,SAASC,GAAE,QAAS,KAAK;AAEzB,eAAe;EACXC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIF,GAAG,CAAC,IAAI;IACtB,MAAMG,KAAI,GAAIH,GAAG,CAAC,IAAI;IACtB,MAAMI,KAAI,GAAIJ,GAAG,CAAC,IAAI;IACtB,MAAMK,IAAG,GAAIL,GAAG,CAAC,IAAI;IACrB,MAAMM,MAAK,GAAIN,GAAG,CAAC,IAAI;IACvB,MAAMO,IAAG,GAAIP,GAAG,CAAC,IAAI;IACrB,MAAMQ,KAAI,GAAIR,GAAG,CAAC,IAAI;IACtB,MAAMS,IAAG,GAAIT,GAAG,CAAC,IAAI;IACrB,MAAMU,QAAO,GAAIV,GAAG,CAAC,IAAI;IACzB,MAAMW,QAAO,GAAIX,GAAG,CAAC;MACDY,SAAS,EAAE,IAAI;MACfC,UAAU,EAAE,IAAI;MAChBC,QAAQ,EAAE,IAAI;MACdC,QAAQ,EAAE,IAAI;MACdC,QAAQ,EAAE,IAAI;MACdV,MAAM,EAAE,IAAI;MACZE,KAAK,EAAE,IAAI;MACXS,WAAW,EAAE;IACjB,CAAC;IAEjB,MAAMC,WAAU,GAAI,YAAY;MAC5B,IAAG